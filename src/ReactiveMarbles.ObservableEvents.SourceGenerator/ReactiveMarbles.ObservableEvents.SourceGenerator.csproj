<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <DevelopmentDependency>true</DevelopmentDependency>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>ReactiveMarbles.ObservableEvents.SourceGenerator</PackageId>
    <NoWarn>$(NoWarn);NU5128;AD0001</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\ReactiveMarbles.ObservableEvents.Roslyn3\ReactiveMarbles.ObservableEvents.Roslyn38.csproj" ReferenceOutputAssembly="false" />
    <ProjectReference Include="..\ReactiveMarbles.ObservableEvents.Roslyn4\ReactiveMarbles.ObservableEvents.Roslyn40.csproj" ReferenceOutputAssembly="false" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\ReactiveMarbles.ObservableEvents.Roslyn3\bin\$(Configuration)\netstandard2.0\ReactiveMarbles.ObservableEvents.Roslyn38.dll" PackagePath="analyzers\dotnet\roslyn3.8\cs" Pack="true" Visible="false" />
    <None Include="..\ReactiveMarbles.ObservableEvents.Roslyn4\bin\$(Configuration)\netstandard2.0\ReactiveMarbles.ObservableEvents.Roslyn40.dll" PackagePath="analyzers\dotnet\roslyn4.0\cs" Pack="true" Visible="false" />
    <None Include="buildTransitive\**\*" Pack="true" PackagePath="buildTransitive" />
  </ItemGroup>
</Project>
